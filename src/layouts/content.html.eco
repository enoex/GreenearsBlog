---
layout: default
---
<div class='page-post'>
    <h1 class='post-title'>
        <%- @document.title or @document.name %>
    </h1>
    <div class='post-date'>
        Posted on <%= @moment(@document.date).format('MMMM Do, YYYY') %>
    </div>

    <div class='post-content'>
        <%- @content %>
    </div>
</div>
